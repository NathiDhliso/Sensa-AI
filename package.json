{
  "name": "memoir-learning-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "deploy": "bash deploy-edge-function.sh",
    "deploy:edge": "supabase functions deploy adk-agents --use-api",
    "logs:edge": "supabase functions logs adk-agents",
    "theme:example": "npm run dev & echo 'Navigate to /examples/theme-usage to see theme examples'",
    "validate:theme": "tsc --noEmit src/styles/themes.ts src/contexts/ThemeContext.tsx",
    "backup:edge": "cp supabase/functions/adk-agents/index.ts supabase/functions/adk-agents/index-backup-$(date +%Y%m%d_%H%M%S).ts",
    "restore:edge": "echo 'Please specify backup file: cp supabase/functions/adk-agents/index-backup-YYYYMMDD_HHMMSS.ts supabase/functions/adk-agents/index.ts'",
    "check:modular": "ls -la supabase/functions/adk-agents/ | grep -E '(constants|types|utils|api|handlers|analysis|features)'",
    "proxy:start": "node cors-proxy-server.js",
    "proxy:dev": "nodemon cors-proxy-server.js"
  },
  "dependencies": {
    "@hpcc-js/wasm": "^2.25.0",
    "@mermaid-js/parser": "^0.5.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/d3": "^7.4.3",
    "@xyflow/react": "^12.7.0",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "mermaid": "^11.8.0",
    "node-fetch": "^2.6.7",
    "pdfjs-dist": "^5.3.93",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flow-renderer": "^10.3.17",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.2",
    "vitest": "^3.2.4"
  }
}
